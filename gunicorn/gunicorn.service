[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/jobfinderapi
Environment="PYTHONPATH=/home/ubuntu/jobfinderapi"
ExecStart=/home/ubuntu/venv/bin/gunicorn --access-logfile - --workers 3 --timeout 300 --bind unix:/run/gunicorn.sock config.wsgi:application


[Install]
WantedBy=multi-user.target
